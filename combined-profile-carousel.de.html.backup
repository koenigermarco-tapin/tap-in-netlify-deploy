        .slider-container {
            padding: 30px 0;
        }

        .slider-value-display {
            text-align: center;
            font-size: 3em;
            font-weight: 700;
            color: #1a365d;
            margin-bottom: 20px;
        }

        .slider {
            width: 100%;
            height: 8px;
            border-radius: 10px;
            background: linear-gradient(90deg, #e0e0e0 0%, #1a365d 100%);
            outline: none;
            -webkit-appearance: none;
        }

        .slider::-webkit-slider-thumb {
            -webkit-appearance: none;
            appearance: none;
            width: 30px;
            height: 30px;
            border-radius: 50%;
            background: #1a365d;
            cursor: pointer;
            box-shadow: 0 2px 8px rgba(169, 50, 38, 0.4);
