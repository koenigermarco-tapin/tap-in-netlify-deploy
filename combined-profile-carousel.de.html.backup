        .slider::-moz-range-thumb {
            width: 30px;
            height: 30px;
            border-radius: 50%;
            background: #1a365d;
            cursor: pointer;
            border: none;
            box-shadow: 0 2px 8px rgba(169, 50, 38, 0.4);
