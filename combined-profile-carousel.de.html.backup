        /* Radio Button Styles (for Leadership questions) */
        .radio-options {
            display: flex;
            flex-direction: column;
            gap: 12px;
        }

        .radio-option {
            position: relative;
            cursor: pointer;
        }

        .radio-option input[type="radio"] {
            position: absolute;
            opacity: 0;
        }

        .radio-option-label {
            display: block;
            padding: 18px 20px 18px 50px;
            background: #f8f9fa;
            border: 2px solid #e0e0e0;
            border-radius: 12px;
            transition: all 0.2s ease;
            font-size: 1.05em;
            line-height: 1.5;
        }

        .radio-option input:checked + .radio-option-label {
            background: #fff5f4;
            border-color: #1a365d;
            font-weight: 600;
        }

        .radio-option-label:hover {
            border-color: #1a365d;
            background: #fff5f4;
        }

        .radio-option-label::before {
            content: '';
            position: absolute;
            left: 18px;
            top: 50%;
            transform: translateY(-50%);
            width: 20px;
            height: 20px;
            border: 2px solid #1a365d;
            border-radius: 50%;
            background: white;
        }

        .radio-option input:checked + .radio-option-label::after {
            content: '';
            position: absolute;
            left: 24px;
            top: 50%;
            transform: translateY(-50%);
            width: 12px;
            height: 12px;
            border-radius: 50%;
            background: #1a365d;
        }

        /* Scale Options (for Worker Type & Mental Health questions) */
        .scale-options {
            display: flex;
            gap: 12px;
            margin-bottom: 20px;
        }

        .scale-option {
            flex: 1;
            text-align: center;
            padding: 20px 10px;
            background: #f8f9fa;
            border: 2px solid #e0e0e0;
            border-radius: 12px;
            cursor: pointer;
            transition: all 0.2s ease;
            position: relative;
        }

        /* Progressive darkening from left (light) to right (dark) */
        .scale-option:nth-child(1) { background: linear-gradient(135deg, #f8f9fa 0%, #f1f3f5 100%); }
        .scale-option:nth-child(2) { background: linear-gradient(135deg, #e9ecef 0%, #dee2e6 100%); }
        .scale-option:nth-child(3) { background: linear-gradient(135deg, #ced4da 0%, #adb5bd 100%); }
        .scale-option:nth-child(4) { background: linear-gradient(135deg, #334155 0%, #c9513f 100%); }
        .scale-option:nth-child(5) { background: linear-gradient(135deg, #1a365d 0%, #0f1f3d 100%); }

        .scale-option:nth-child(4) span,
        .scale-option:nth-child(5) span,
        .scale-option:nth-child(4) small,
        .scale-option:nth-child(5) small {
            color: white;
        }

        .scale-option span {
            display: block;
            font-size: 1.8em;
            font-weight: 700;
            color: #333;
            margin-bottom: 8px;
        }

        .scale-option small {
